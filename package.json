{
  "name": "vue3use",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://github.com/aesoper101/vue3use#readme",
  "bugs": {
    "url": "https://github.com/aesoper101/vue3use/issues"
  },
  "author": {
    "name": "aesoper",
    "email": "weilanzhuan@163.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aesoper101/vue3use.git"
  },
  "workspaces": [
    "packages/*",
    "build/*/**"
  ],
  "scripts": {
    "build": "yarn build:lib && yarn build:cli && yarn build:plugin",
    "build:cli": "lerna run --scope \"@aesoper/*-cli\" build",
    "build:lib": "lerna run --ignore \"@aesoper/*-cli\" --ignore \"@aesoper/vite-*\" build",
    "build:plugin": "lerna run --scope \"@aesoper/vite-*\" build",
    "postinstall": "husky install",
    "lintstaged": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,cjs,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{less,css}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@rushstack/eslint-patch": "^1.6.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.10.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.4.0",
    "cypress": "^13.6.1",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-vue": "^9.19.2",
    "execa": "^8.0.1",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "lerna": "^8.0.0",
    "lint-staged": "^15.2.0",
    "postcss-html": "^1.5.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "stylelint": "^15.11.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "typescript": "^5.3.3",
    "vite": "^5.0.6",
    "vitest": "^1.0.1",
    "zx": "^7.2.3"
  },
  "packageManager": "yarn@4.0.2",
  "engines": {
    "node": "^18.0.0 || >= 20.0.0"
  },
  "readme": "ERROR: No README data found!",
  "_id": "vue3use@1.0.0"
}
