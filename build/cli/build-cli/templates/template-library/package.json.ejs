{
  "name": "<%= packageName %>",
  "version": "1.0.0",
  "description": "> TODO: description",
  "author": {
    "name": "<%= gitUser %>",
    "email": "<%= gitEmail %>"
  },
  "homepage": "",
  "license": "MIT",
  "type": "module",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "unpkg": "./dist/<%= unpkgName %>.min.js",
  "exports": {
    ".": {
      "import": "./es/index.js",
      "require": "./lib/index.js",
      "types": "./es/index.d.ts"
    }<% if (style) { %>,
    "./*": "./*"<% } %>
  },
  "files": [
    "dist",
    "es",
    "lib"
  ],
<% if (style) { %>
  "sideEffects": [
    "dist/*",
    "es/**/style/*",
    "lib/**/style/*",
    "*.less"
  ],
<% } %>
  "repository": {
    "type": "git",
    "url": "<%= gitURL %>",
    "directory": "<%= repositoryDir %>"
  },
  "scripts": {
    "build": "yarn run build:library && <% if (style) { %> yarn run build:style && <% } %>yarn run dtsgen",
    "build:library": "vis-cli build:library -u",
    "dtsgen": "vis-cli dtsgen 'src/**/*.{ts,tsx,vue}' -o es",
<% if (style) { %>
    "build:style": "yarn run lessgen && vis-cli build:style",
    "lessgen": "vis-cli lessgen",
<% } %>
    "test": "vitest run --coverage"
  },
  "devDependencies": {
    "@beedt/vis-cli": "workspace:^",
    "@tsconfig/node18": "^18.2.2",
    "@vue/tsconfig": "^0.4.0"
  }
}
